<div class="container">
  <article>
    <form action="/api/sessions/out_login" method="post">
      <label for="email">
        <input type="email" name="email" id="email" required />
      </label>

      <label for="password">
        <input type="password" name="password" id="password" required />
      </label>

      <button>Log in</button>
      <a href="/register">Register</a>
    </form>
    <a style="border-radius: 17px;" href="/api/sessions/github" role="button">
      <img
        width="60"
        height="60"
        src="https://img.icons8.com/ios-glyphs/60/github.png"
        alt="github"
      />
      Login with GitHub
    </a>
  </article>

</div>