<div class="container">
 
  <div class="grid">
    <div>
      <h2><strong>Total Pages: {{totalPages}}</strong></h2>
    </div>
    <div>
      <h2><strong>Now: {{page}}</strong></h2>
    </div>
    <div>
      <a href="{{prevPage}}"  {{#unless hasPrevPage}} disabled {{/unless}}  role="button" >Prev </a>
      <a href="{{nextPage}}" {{#unless hasNextPage}} disabled {{/unless}}   role="button" >Next </a>
    </div>
  </div>
 
 
  <table>
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">Description</th>
      <th scope="col">Stock</th>
      <th scope="col">Price</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
 
  {{#each payload}}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.category}}</td>
      <td>{{this.description}} </td>
      <td>{{this.stock}} </td>
      <td>{{this.price}}</td>
      <td> <button  onclick="addToCart('{{this._id}}')"> Add to cart </button> </td>
     
    </tr>
 
    {{/each}}
  </tbody>
</table>
 
 
</div>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="js/products.js"></script>